---
import Instagram from "../components/svg/Instagram.astro";
const { img } = Astro.props;
---

<section>

    <img src={img} alt="instagram billede">
      <div class="centered">
<Instagram /></div>
</section>


<style>
img{
  width: 100%;
  height: 100%;
}
  
  img:hover{
        opacity: 50%;
    }
section{
  position: relative;
  text-align: center;
}
.centered {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);

  
}


    </style>